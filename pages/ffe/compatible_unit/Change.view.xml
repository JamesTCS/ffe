<core:View xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.ui.commons" 
	xmlns:m= "sap.m"
	xmlns:ui="sap.ca.ui"
	xmlns:l="sap.ui.layout"
	xmlns:t="sap.ui.table"
	xmlns:f="sap.ui.layout.form"
	controllerName="pge.ffe.pages.ffe.compatible_unit.Change" 
	xmlns:html="http://www.w3.org/1999/xhtml">
	
	<html:h1 class="center">Change Compatible Unit</html:h1>
	<l:HorizontalLayout class="float-right savePane">
		<Button text="Back"  press="back" class="right10"/>
		<Button text="Save" icon="images/save_small.png" press="save" class="right10"/>
		<Button text="Save &amp; Exit" press="saveAndExit" class="orangeButton"/>
	</l:HorizontalLayout>
		<l:VerticalLayout class="top25">
		<html:div class="bgBlue">
			<l:HorizontalLayout>
				<Label text="CU ID:" class="spacing" /><TextField class="right10 top10" value="SW-U-600-RAM-20" />
				<Button icon="/images/struclist_small.png"  press="back" class="right10 top10"/>
				<Label text="Description:" class="spacing left25" /><TextField value="SW UG SF6 SUB BYPASS 4-WAY 600A 27KV" class="right10 top10" />
				<Label text="Status:" class="spacing left25" /><TextField value="CUCR" class="right10 top10" />
			</l:HorizontalLayout>
		
			<l:HorizontalLayout>
				<Label text="Base Unit:" class="spacing" /><TextField value="EA" class="right10 top10" />
				<Label text="Valid to:" class="spacing left105" /><TextField value="31129999" class="right10 top10" />
				<Label text="Valid From:" class="spacing left25" /><TextField value="09092015" class="right10 top10" />
			</l:HorizontalLayout>
			
			<l:HorizontalLayout>
				<Label text="Task List:" class="spacing " /><TextField value="CUT00002" class="right10 top10 bottom10" />
				<Button text="Create" icon="/images/new_small.png" class="left5 top10 bottom10" press="toNewTaskList"/>
				<Button text="Change" icon="/images/edit_small.png" class="left5 top10 bottom10" press="toNewTaskList" /> 
				<Button text="Display" icon="/images/view_small.png" class="left5 top10 bottom10" press="toNewTaskList"/>
			</l:HorizontalLayout>
			</html:div>
			<html:div class="bgBeige">
			<l:HorizontalLayout>
				<Label text="Object Reference:" class="spacing " /><TextField value="02" class=" right10 top10" />
			</l:HorizontalLayout>
			
			<l:HorizontalLayout>
				<Label text="Function Location:" class="spacing" /><TextField class="right10 top10" />
				<Button text="Create" icon="/images/new_small.png" class="left25 top10" press="toCreateFL" />
				<Button text="Change" icon="/images/edit_small.png" class="left10 top10" press="toCreateFL" /> 
				<Button text="Display" icon="/images/view_small.png" class="left10 top10" press="toCreateFL"/>
			<!--<Button text="Where Used" icon="/images/view_small.png" class="left10 top10 blueButton" press=""/>-->
			</l:HorizontalLayout>
			
			<l:HorizontalLayout class="bottom10">
				<Label text="Equipment:" class="spacing" /><TextField value="CUREF-000000000001" class="right10 top10" />
				<Button text="Create" icon="/images/new_small.png" class="left25 top10" press="toDisplayMaterial"/>
				<Button text="Change" icon="/images/edit_small.png" class="left10 top10" press="toDisplayMaterial" /> 
				<Button text="Display" icon="/images/view_small.png" class="left10 top10" press="toDisplayMaterial"/>
				<!--<Button text="Where Used" icon="/images/view_small.png" class="left10 top10 blueButton" press=""/>-->
			</l:HorizontalLayout>
	        </html:div>
			<!-- SubItems Table -->
		    <l:HorizontalLayout class="float-right">
				<Button  text="CU Clasification" press="toCreateClassification" class="buttonOrange"/>
				<Button  text="Display Material" press="toDisplayMaterial"/>
		    </l:HorizontalLayout>
		<html:div class="bgBlue">
		
		<t:Table id="subItemsTable" class="top25" >
		<t:extension>
		
		</t:extension>
		<t:title>
		<Label text="Sub Items" class="spacing" />
		</t:title>
			<!--
			<t:columns>
				<t:Column name="#"><t:label><Label text="#"/></t:label></t:Column>
				<t:Column name="cu"><t:label><Label text="CU"/></t:label></t:Column>
				<t:Column name="Description"><t:label><Label text="CU"/></t:label></t:Column>
			</t:columns>
			-->
		</t:Table>

		<!-- Table Buttons -->
		<l:HorizontalLayout class="top10">
			<Button text="Add Item" class="blueButton" />  
			<Button text="Delete Item" class="left10 blueButton" />
			<Button text="Copy"  class="left10 blueButton" />
			<Button text="Lock"  class="left10 blueButton" press="" /> 
			<Button text="Planned/Actual Cost Comparision"  class="left10 blueButton" press=""/>
		</l:HorizontalLayout>
		</html:div>
	</l:VerticalLayout>
</core:View>